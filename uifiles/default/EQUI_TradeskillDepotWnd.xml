<?xml version = "1.0"?>
<XML ID="EQInterfaceDefinitionLanguage">
	<Schema xmlns="EverQuestData" xmlns:dt="EverQuestDataTypes"/>
	
	<Label item="TD_NoDataLabel">
		<ScreenID>TD_NoDataLabel</ScreenID>
		<AutoStretch>true</AutoStretch>
		<BottomAnchorToTop>false</BottomAnchorToTop>
		<RightAnchorToLeft>false</RightAnchorToLeft>
		<TopAnchorOffset>28</TopAnchorOffset>
		<BottomAnchorOffset>50</BottomAnchorOffset>
		<LeftAnchorOffset>5</LeftAnchorOffset>
		<RightAnchorOffset>5</RightAnchorOffset>
		<Text>Please wait while your items are loaded.</Text>
		<Font>5</Font>
	</Label>
	
	<Screen item="TD_ScreenHideList">
		<ScreenID>TD_ScreenHideList</ScreenID>
		<AutoStretch>true</AutoStretch>
		<RelativePosition>true</RelativePosition>
		<BottomAnchorToTop>false</BottomAnchorToTop>
		<RightAnchorToLeft>false</RightAnchorToLeft>
		<DrawTemplate>WDT_Inner</DrawTemplate>
		<Style_Titlebar>false</Style_Titlebar>
		<Style_Closebox>false</Style_Closebox>
		<Style_Minimizebox>false</Style_Minimizebox>
		<Pieces>TD_NoDataLabel</Pieces>
	</Screen>

	<Listbox item="TD_Item_List">
		<ScreenID>TD_Item_List</ScreenID>
		<DrawTemplate>WDT_Inner</DrawTemplate>
		<AutoStretch>true</AutoStretch>
		<BottomAnchorToTop>false</BottomAnchorToTop>
		<RightAnchorToLeft>false</RightAnchorToLeft>
		<TopAnchorOffset>28</TopAnchorOffset>
		<BottomAnchorOffset>50</BottomAnchorOffset>
		<LeftAnchorOffset>5</LeftAnchorOffset>
		<RightAnchorOffset>5</RightAnchorOffset>
		<Style_Border>true</Style_Border>
		<Style_VScroll>true</Style_VScroll>
		<Columns>
			<Width>24</Width>
			<Heading></Heading>
		</Columns>
		<Columns>
			<Width>200</Width>
			<Heading>Item Name</Heading>
		</Columns>
		<Columns>
			<Width>100</Width>
			<Heading>Qty</Heading>
		</Columns>
	</Listbox>

	<Button item="TD_Inspect_Button">
		<ScreenID>TD_Inspect_Button</ScreenID>
		<AutoStretch>true</AutoStretch>
		<TopAnchorToTop>false</TopAnchorToTop>
		<BottomAnchorToTop>false</BottomAnchorToTop>
		<LeftAnchorToLeft>false</LeftAnchorToLeft>
		<RightAnchorToLeft>false</RightAnchorToLeft>
		<TopAnchorOffset>25</TopAnchorOffset>
		<BottomAnchorOffset>5</BottomAnchorOffset>
		<LeftAnchorOffset>138</LeftAnchorOffset>
		<RightAnchorOffset>74</RightAnchorOffset>
		<TooltipReference>Click here to inspect the selected item (R-Click)</TooltipReference>
		<Text>Inspect</Text>
		<Template>BDT_Normal</Template>
	</Button>

	<Button item="TD_Selected_Item_Slot">
		<ScreenID>TD_Selected_Item_Slot</ScreenID>
		<AutoStretch>true</AutoStretch>
		<TopAnchorToTop>false</TopAnchorToTop>
		<BottomAnchorToTop>false</BottomAnchorToTop>
		<TopAnchorOffset>45</TopAnchorOffset>
		<BottomAnchorOffset>3</BottomAnchorOffset>
		<LeftAnchorOffset>79</LeftAnchorOffset>
		<RightAnchorOffset>121</RightAnchorOffset>
		<Template>BDT_DragItem</Template>
		<DecalOffset>
			<X>2</X>
			<Y>2</Y>
		</DecalOffset>
		<DecalSize>
			<CX>40</CX>
			<CY>40</CY>
		</DecalSize>
	</Button>

	<Label item="TD_Selected_Item_Label">
		<ScreenID>TD_Selected_Item_Label</ScreenID>
		<AutoStretch>true</AutoStretch>
		<TopAnchorToTop>false</TopAnchorToTop>
		<BottomAnchorToTop>false</BottomAnchorToTop>
		<RightAnchorToLeft>false</RightAnchorToLeft>
		<TopAnchorOffset>43</TopAnchorOffset>
		<BottomAnchorOffset>5</BottomAnchorOffset>
		<LeftAnchorOffset>125</LeftAnchorOffset>
		<RightAnchorOffset>85</RightAnchorOffset>
		<Text>Selected Item</Text>
	</Label>

	<Button item="TD_Withdraw_Button">
		<ScreenID>TD_Withdraw_Button</ScreenID>
		<AutoStretch>true</AutoStretch>
		<TopAnchorToTop>false</TopAnchorToTop>
		<BottomAnchorToTop>false</BottomAnchorToTop>
		<TopAnchorOffset>45</TopAnchorOffset>
		<BottomAnchorOffset>5</BottomAnchorOffset>
		<LeftAnchorOffset>5</LeftAnchorOffset>
		<RightAnchorOffset>69</RightAnchorOffset>
		<TooltipReference>Click here to withdraw the selected item</TooltipReference>
		<Text>Withdraw</Text>
		<Template>BDT_Normal</Template>
	</Button>

	<Button item="TD_Exit_Button">
		<ScreenID>TD_Exit_Button</ScreenID>
		<AutoStretch>true</AutoStretch>
		<TopAnchorToTop>false</TopAnchorToTop>
		<BottomAnchorToTop>false</BottomAnchorToTop>
		<LeftAnchorToLeft>false</LeftAnchorToLeft>
		<RightAnchorToLeft>false</RightAnchorToLeft>
		<TopAnchorOffset>25</TopAnchorOffset>
		<BottomAnchorOffset>5</BottomAnchorOffset>
		<LeftAnchorOffset>69</LeftAnchorOffset>
		<RightAnchorOffset>5</RightAnchorOffset>
		<TooltipReference>Click here to close the Tradeskill Depot window</TooltipReference>
		<Text>Exit</Text>
		<Template>BDT_Normal</Template>
	</Button>

	<Label item="TD_Item_Count_Label">
		<ScreenID>TD_Item_Count_Label</ScreenID>
		<Location>
			<X>5</X>
			<Y>7</Y>
		</Location>
		<Size>
			<CX>75</CX>
			<CY>14</CY>
		</Size>
		<Text>Total Count:</Text>
		<NoWrap>true</NoWrap>
	</Label>

	<Label item="TD_Item_Count_Number">
		<ScreenID>TD_Item_Count_Number</ScreenID>
		<Location>
			<X>75</X>
			<Y>7</Y>
		</Location>
		<Size>
			<CX>70</CX>
			<CY>14</CY>
		</Size>
		<Text>0 / 0</Text>
		<NoWrap>true</NoWrap>
	</Label>
	
	<Editbox item="TD_SearchItem_Input">
		<ScreenID>TD_SearchItem_Input</ScreenID>
		<DrawTemplate>WDT_Inner</DrawTemplate>
		<RelativePosition>true</RelativePosition>
		<AutoStretch>true</AutoStretch>
		<LeftAnchorToLeft>false</LeftAnchorToLeft>
		<RightAnchorToLeft>false</RightAnchorToLeft>
		<TooltipReference>Search by partial item name.</TooltipReference>
		<LeftAnchorOffset>220</LeftAnchorOffset>
		<TopAnchorOffset>4</TopAnchorOffset>
		<RightAnchorOffset>82</RightAnchorOffset>
		<BottomAnchorOffset>24</BottomAnchorOffset>
		<Style_Border>true</Style_Border>
	</Editbox>
	
	<Button item = "TD_SearchItem_Button">
		<ScreenID>TD_SearchItem_Button</ScreenID>
		<RelativePosition>true</RelativePosition>
		<AutoStretch>true</AutoStretch>
		<LeftAnchorToLeft>false</LeftAnchorToLeft>
		<RightAnchorToLeft>false</RightAnchorToLeft>
		<Style_Checkbox>false</Style_Checkbox>
		<Style_Transparent>false</Style_Transparent>	
		<LeftAnchorOffset>80</LeftAnchorOffset>
		<TopAnchorOffset>4</TopAnchorOffset>
		<RightAnchorOffset>5</RightAnchorOffset>
		<BottomAnchorOffset>24</BottomAnchorOffset>
		<Text>Search</Text>
		<BackgroundTextureTint>
			<R>0</R>
			<G>158</G>
			<B>255</B>
		</BackgroundTextureTint>
		<NoWrap>true</NoWrap>
		<DecalSize>
			<CX>16</CX>
			<CY>16</CY>
		</DecalSize>
		<DecalOffset>
			<X>50</X>
			<Y>2</Y>
		</DecalOffset>
		<TextAlignCenter>false</TextAlignCenter>
		<TextOffsetX>10</TextOffsetX>
		<Template>BDT_FindItem</Template>
	</Button>

	<Screen item="TradeskillDepotWnd">
		<RelativePosition>false</RelativePosition>
		<Location>
			<X>0</X>
			<Y>25</Y>
		</Location>
		<Size>
			<CX>398</CX>
			<CY>400</CY>
		</Size>
		<Text>Tradeskill Depot</Text>
		<DrawTemplate>WDT_Def</DrawTemplate>
		<Style_Titlebar>true</Style_Titlebar>
		<Style_Qmarkbox>true</Style_Qmarkbox>
		<Style_Closebox>true</Style_Closebox>
		<Style_Minimizebox>false</Style_Minimizebox>
		<Style_Border>true</Style_Border>
		<Style_Sizable>true</Style_Sizable>
		<Pieces>Screen:TD_ScreenHideList</Pieces>
		<Pieces>TD_Selected_Item_Label</Pieces>
		<Pieces>TD_Selected_Item_Slot</Pieces>
		<Pieces>TD_Exit_Button</Pieces>
		<Pieces>TD_Withdraw_Button</Pieces>
		<Pieces>TD_Item_Count_Label</Pieces>
		<Pieces>TD_Item_Count_Number</Pieces>
		<Pieces>TD_Inspect_Button</Pieces>
		<Pieces>TD_Item_List</Pieces>
		<Pieces>TD_SearchItem_Input</Pieces>
		<Pieces>TD_SearchItem_Button</Pieces>
	</Screen>


</XML>